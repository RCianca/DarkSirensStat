  RowBox[{"EE", "[", "z_", "]"}], ":=", 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"\[CapitalOmega]m", "*", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", "z"}], ")"}], "3"]}], "+", 
     RowBox[{"\[CapitalOmega]r", "*", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", "z"}], ")"}], "4"]}], "+", 
     RowBox[{"\[CapitalOmega]k", "*", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", "z"}], ")"}], "2"]}], "+", "\[CapitalOmega]l"}], 
    ")"}], 
   RowBox[{"1", "/", "2"}]]}]], "Input",
 CellChangeTimes->{{3.858055660094289*^9, 3.858055782741208*^9}},
 CellLabel->"In[9]:=",ExpressionUUID->"48d4131c-e48c-4418-8596-810917890357"],

